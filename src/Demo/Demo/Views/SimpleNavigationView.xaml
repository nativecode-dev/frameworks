<?xml version="1.0" encoding="utf-8"?>

<ContentPage xmlns="http://xamarin.com/schemas/2014/forms" xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Demo.Views.SimpleNavigationView" IsBusy="{Binding IsBusy}" Title="{Binding Title}">
  <StackLayout Padding="20">
    <Button Command="{Binding ChooseStyleCommand}" Text="Choose App Style" />
    <Button Command="{Binding ArticleCommand}" Text="View Article" />
    <Button Command="{Binding WebBrowserCommand}" Text="Web Browser" />
  </StackLayout>
</ContentPage>
